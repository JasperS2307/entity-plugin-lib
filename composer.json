{
    "name": "hostnet/entity-plugin-lib",
    "type": "composer-plugin",
    "description": "Installer for all the Hostnet Doctrine entity libraries",
    "license": "proprietary",
    "require": {
        "php": ">=5.4",
        "twig/twig": ">=1.13",
        "composer-plugin-api": "1.0.0"
    },
    "require-dev": {
        "composer/composer": "master@dev",
        "phpunit/phpunit": "4.*",
        "squizlabs/php_codesniffer": "1.*"
    },
    "minimum-stability": "stable",
    "repositories": [
        {
            "type":"composer",
            "url": "http://packagist.hostnetbv.nl"
        }
    ],
    "autoload": {
        "psr-4": { "Hostnet\\Component\\EntityPlugin\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Hostnet\\Component\\EntityPlugin\\": "test/" }
    },
    "config": {
        "github-oauth": {
            "github.com": "8ed444290f5ae4c7e82628ef30acc90680f0474b"
        }
    },
    "extra": {
        "class": "Hostnet\\Component\\EntityPlugin\\Plugin"
    }
}
