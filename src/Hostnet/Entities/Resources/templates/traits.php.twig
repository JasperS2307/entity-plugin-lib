<?php
namespace {{ namespace }};

{% for generated_namespace, alias in use_statements %}
use {{ generated_namespace }}\{{ class_name }}Trait as {{ alias }};
{% endfor %}

/**
 * Trait of traits generated for {{ class_name }}
 * This is the guy that the main class needs to require, and ensures that everything is glued
 * together
 */
trait {{ class_name }}Traits
{
    {% for alias in use_statements %}
    use {{ alias }};
    {% endfor %}
}
